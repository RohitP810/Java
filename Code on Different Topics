//create a interface vehicle with 3 methods void gearChange(int x) void speedIncrease(int x) void break(int x) implement this interface in class byclycle,bike and car you can create ur own datamebers and member function

/*import java.util.*;

interface vehicle
{
	void gearChange(int x);
	void speedIncrease(int x);
	void Break(int x);
}

class bycycle implements vehicle
{
	int gear = 0;
	int speed = 0;
	public void gearChange(int x) 
	{
		gear += x;
		if(x>0)
			System.out.println("Cycle Gear increased!!!\nGear: "+gear+"\nSpeed: "+speed);
		else
			System.out.println("Cycle Gear decreased!!!\nGear: "+gear+"\nSpeed: "+speed);
		if(gear == 0 && x<0)
			System.out.println("Cycle Gear cant be decreased!!!\nGear: "+gear+"\nSpeed: "+speed);
	}
	
	public void speedIncrease(int x) 
	{
		speed +=x;
		System.out.println("Cycle Speed increased!!!\nGear: "+gear+"\nSpeed: "+speed);
		if(gear == 0)
			System.out.println("Cycle Increase the gear first!!!\nGear: "+gear+"\nSpeed: "+speed);
	}
	
	public void Break(int x) 
	{
		speed -= x;
		System.out.println("Cycle Speed decreased by "+x+" !!!\nGear: "+gear+"\nSpeed: "+speed);
	}
}

class bike implements vehicle
{
	int gear = 0;
	int speed = 0;
	public void gearChange(int x) 
	{
		gear += x;
		if(x>0)
			System.out.println("Bike Gear increased!!!\nGear: "+x+"\nSpeed: "+speed);
		else
			System.out.println("Bike Gear decreased!!!\nGear: "+x+"\nSpeed: "+speed);
		if(gear == 0 && x<0)
			System.out.println("Bike Gear cant be decreased!!!\nGear: "+x+"\nSpeed: "+speed);
	}
	
	public void speedIncrease(int x) 
	{
		speed +=x;
		System.out.println("Bike Speed increased!!!\nGear: "+x+"\nSpeed: "+speed);
		if(gear == 0)
			System.out.println("Bike Increase the gear first!!!\nGear: "+x+"\nSpeed: "+speed);
	}
	
	public void Break(int x) 
	{
		speed -= x;
		System.out.println("Bike Speed decreased by "+x+" !!!\nGear: "+x+"\nSpeed: "+speed);
	}
}

class car implements vehicle
{
	int gear = 0;
	int speed = 0;
	public void gearChange(int x) 
	{
		gear += x;
		if(x>0)
			System.out.println("Car Gear increased!!!\nGear: "+x+"\nSpeed: "+speed);
		else
			System.out.println("Car Gear decreased!!!\nGear: "+x+"\nSpeed: "+speed);
		if(gear == 0 && x<0)
			System.out.println("Car Gear cant be decreased!!!\nGear: "+x+"\nSpeed: "+speed);
	}
	
	public void speedIncrease(int x) 
	{
		speed +=x;
		System.out.println("Car Speed increased!!!\nGear: "+x+"\nSpeed: "+speed);
		if(gear == 0)
			System.out.println("Car Increase the gear first!!!\nGear: "+x+"\nSpeed: "+speed);
	}
	
	public void Break(int x) 
	{
		speed -= x;
		System.out.println("Car Speed decreased by "+x+" !!!\nGear: "+x+"\nSpeed: "+speed);
	}
}

public class test 
{
	  public static void main(String[] args) 
	  {
		  bycycle bc = new bycycle();
		  bike bk = new bike();
		  car c = new car();
		  
		  bc.gearChange(1);
		  bc.speedIncrease(5);
		  bc.Break(5);
		  bc.gearChange(-1);
		  
		  bk.gearChange(1);
		  bk.speedIncrease(5);
		  bk.Break(5);
		  bk.gearChange(-1);
		  
		  c.gearChange(1);
		  c.speedIncrease(20);
		  c.Break(5);
		  c.gearChange(1);
		  c.speedIncrease(30);
		  c.Break(30);
		  c.gearChange(-2);
	  }
}*/


/*import java.util.*;

interface add_sub
{
	void add(double x, double y);
	void sub(double x, double y);
}

interface mull_div
{
	void mul(double x, double y);
	void div(double x, double y);
}

interface calculator extends add_sub,mull_div
{
	void printResult(double res);
}

class mycalc implements calculator
{
	public void add(double x, double y)
	{
		System.out.print(x + "+" + y);
		printResult(x+y);
	}
	public void sub(double x, double y)
	{
		System.out.print(x + "-" + y);
		printResult(x-y);
	}
	public void mul(double x, double y)
	{
		System.out.print(x + "*" + y);
		printResult(x*y);
	}
	public void div(double x, double y)
	{
		System.out.print(x + "/" + y);
		printResult(x/y);
	}
	public void printResult(double res)
	{
		System.out.println(" = "+res);
	}
}

public class test 
{
	  public static void main(String[] args) 
	  {
		  mycalc c = new mycalc();
		  c.add(5, 5);
		  c.sub(5, 5);
		  c.mul(5, 5);
		  c.div(5, 5);
	  }
}*/


