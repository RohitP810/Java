public class test {

    public static void main(String[] args) {
            
        Scanner in = new Scanner(System.in);
        float arr[]=new float[3],sum=0;
        float avg;
        for(int i=0;i<3;i++)
        {
            arr[i]=in.nextInt();
        }
        for(int i=0;i<3;i++)
        {
            sum+=arr[i];
        }
        avg=sum/3;
        if(sum%3==0)
        {
            System.out.format("%.1f",avg);    
        }
        else{
            String num = String.format("%.2f", avg);
            num = num.substring(0, num.length() - 1);
            System.out.format(num);
        }
    }
